GET         /                              @controllers.Index.index
POST        /github/                       @controllers.github.EventServer.push
GET         /github/:owner/:repo           @controllers.github.EventServer.watch(owner: String, repo: String)
POST        /bitbucket/                    @controllers.github.EventServer.push
GET         /bitbucket/:owner/:repo        @controllers.github.EventServer.watch(owner: String, repo: String)
GET         /github-callback/              @controllers.github.InstallHooks.githubCallback
GET         /install/                      @controllers.github.InstallHooks.installGet
POST        /install/                      @controllers.github.InstallHooks.installPost
GET         /static/*name                  @controllers.StaticDev.get(name: String)
GET         /version/                      @controllers.Version.version
GET         /install-Bitbucket/            @controllers.github.InstallHooks.installGetBitbucket

